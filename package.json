{
  "name": "base-conv",
  "version": "1.1.0",
  "description": "Convert arbitrarily big numbers from any radix representation to any other",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "test": "nyc node test",
    "prepare": "rollup -c",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": "https://github.com/caub/base-conv.git",
  "keywords": [
    "number",
    "radix",
    "base",
    "convert"
  ],
  "author": "caub",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caub/base-conv/issues"
  },
  "homepage": "https://github.com/caub/base-conv#readme",
  "nyc": {
    "extension": [
      ".mjs"
    ],
    "require": [
      "@std/esm"
    ],
    "check-coverage": true
  },
  "devDependencies": {
    "@std/esm": "^0.26.0",
    "codecov": "^3.0.2",
    "nyc": "^12.0.2",
    "rollup": "^0.58.2"
  }
}
